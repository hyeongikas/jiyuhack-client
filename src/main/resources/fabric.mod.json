{
  "schemaVersion": 1,
  "id": "jiyuhack",
  "version": "${version}",

  "name": "JiyuHack",
  "description": "Minecraft Griefing Utility Hack",
  "authors": [
    "Jiyu + SeoJun"
  ],
  "contact": {
    "homepage": "https://jiyubear.co/",
    "issues": "https://jiyubear.co/",
    "sources": "https://jiyubear.co/"
  },

  "license": "GPL-3.0-or-later",
  "icon": "assets/wurst/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "net.wurstclient.WurstInitializer"
    ]
  },
  "mixins": [
    "wurst.mixins.json"
  ],
  "accessWidener" : "wurst.accesswidener",

  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric-api": ">=0.91.1",
    "minecraft": "~1.20.3-beta.3",
    "java": ">=17"
  },
  "suggests": {
    "mo_glass": "*"
  },
  "breaks": {
    "wi_zoom": "*",
    "wi-zoom": "*",
    "vulkanmod": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Wiki": "https://jiyubear.co/",
        "Forum": "https://jiyubear.co/",
        "Donate": "https://jiyubear.co/"
      }
    }
  }
}
